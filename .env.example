
# Supabase Configuration (Required for authentication and database)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database URL for Prisma (same as Supabase but with proper format)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Google Analytics (Optional - for website analytics)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Google Drive API (Optional - for exporting data to Google Sheets)
VITE_GOOGLE_DRIVE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_DRIVE_API_KEY=your_google_api_key

# Excel Export API (Optional - for advanced Excel exports)
VITE_EXCEL_API_KEY=your_excel_api_key

# Jupyter Notebook Integration (Optional)
VITE_JUPYTER_HUB_URL=your_jupyter_hub_url
VITE_JUPYTER_TOKEN=your_jupyter_token

# Email Service (Optional - for notifications and reports)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_FROM_EMAIL=noreply@yourapp.com

# Admin Configuration
VITE_ADMIN_EMAILS=admin@yourapp.com,manager@yourapp.com

# Environment
VITE_ENVIRONMENT=development
